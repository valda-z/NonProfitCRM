@model string
<script>
    $(function () {
        function pageLoad() {
            $('.widget').widgster();
            //init parsley for pjax requests
            $('#@Model').parsley();
            //select2
            $(".select2").each(function () {
                $(this).select2($(this).data());
            });
        }
        pageLoad();
        SingApp.onPageLoad(pageLoad);
    });
</script>


